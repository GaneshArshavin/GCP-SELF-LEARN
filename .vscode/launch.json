  "version": "0.2.0",
  "configurations": [
    
    {
      "type": "cloudcode",
      "language": "NETCore",
      "request": "launch",
      "debugPort": 80,
      "localRoot": "\"${workspaceFolder}\"",
      "remoteRoot": "/app",
      "name": "Launch App on Kubernetes Cluster: .NET Core",
      "podSelector": {
        "app": "deployment-name"
      }
    },
    {
      "type": "cloudcode",
      "language": "NETCore",
      "request": "attach",
      "debugPort": 80,
      "localRoot": "\"${workspaceFolder}\"",
      "remoteRoot": "/app",
      "name": "Attach App on Kubernetes Cluster: .NET Core",
      "podSelector": {
        "app": "deployment-name"
      }
    }
  ]
}